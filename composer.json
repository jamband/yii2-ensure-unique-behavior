{
  "name": "jamband/yii2-ensure-unique-behavior",
  "description": "This extension insert unique identifier automatically for the Yii 2 framework",
  "keywords": ["yii2", "extension", "behavior", "unique"],
  "homepage": "https://github.com/jamband/yii2-ensure-unique-behavior",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomoki Morita",
      "email": "tmsongbooks215@gmail.com",
      "homepage": "https://jamband.github.io/"
    }
  ],
  "require": {
    "php": "^7.2",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "provide": {
    "bower-asset/jquery": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*"
  },
  "autoload": {
    "psr-4": {
      "jamband\\behaviors\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  }
}
